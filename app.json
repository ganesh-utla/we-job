{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "name": "WeJob",
    "slug": "react_native_jobs",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafafc"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cc033e54-5272-477c-946c-873ecb7fd01c"
      }
    },
    "android": {
      "package": "com.ganesh_utla.react_native_jobs",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fafafc"
      }
    }
  }
}
